<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Background colors-->
    <Color x:Key="DarkGreenColor">#1C3E42</Color>
    <Color x:Key="DarkBlueColor">#152F4D</Color>
    <Color x:Key="DarkGrayColor">#1E1E1E</Color>
    <Color x:Key="LightGrayColor">#DEDEDE</Color>
    <SolidColorBrush x:Key="LightGrayColorBrush" Color="{StaticResource LightGrayColor}"/>

    <!--Window border colors-->
    <Color x:Key="GreenColor">#4AE17F</Color>
    <Color x:Key="BlueColor">#29B5F7</Color>
    <SolidColorBrush x:Key="GreenColorBrush" Color="{StaticResource GreenColor}"/>
    <SolidColorBrush x:Key="BlueColorBrush" Color="{StaticResource BlueColor}"/>
    <SolidColorBrush x:Key="DarkGreenColorBrush" Color="{StaticResource DarkGreenColor}"/>
    <SolidColorBrush x:Key="DarkBlueColorBrush" Color="{StaticResource DarkBlueColor}"/>
    <SolidColorBrush x:Key="DarkGrayColorBrush" Color="{StaticResource DarkGrayColor}"/>
    <Color x:Key="LightGreenColor">#ACE1BF</Color>
    <SolidColorBrush x:Key="LightGreenColorBrush" Color="{StaticResource LightGreenColor}"/>

    <!--Foreground colors-->
    <SolidColorBrush x:Key="SignInWindowGrayColorBrush" Color="#708A97"/>
    <Color x:Key="WhiteColor">#F1FEFA</Color>
    <SolidColorBrush x:Key="WhiteColorBrush" Color="{StaticResource WhiteColor}"/>


    <Color x:Key="MenuButtonsText">White</Color>
    
</ResourceDictionary>